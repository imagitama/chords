{
  "name": "chord.guide",
  "description": "A React app that shows chords, keys and more.",
  "version": "1.0.0",
  "homepage": "http://imagitama.github.io/chordmaster",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@sentry/browser": "^4.6.6",
    "emotion-theming": "^10.0.10",
    "guitar-chord-definitions": "^1.0.2",
    "moment": "^2.24.0",
    "query-string": "^6.4.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.1",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx}",
    "ultimate-guitar-converter": "node tools/ultimate-guitar-converter/main.js",
    "prettier": "prettier src/**/*.{js,jsx}",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "react/prop-types": false,
      "quote-props": [
        "error",
        "as-needed"
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "gh-pages": "^3.0.0",
    "jest-emotion": "^10.0.10",
    "jest-fetch-mock": "^2.1.2",
    "prettier": "^1.18.2"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "src/**/*/*.{js,jsx}"
    ],
    "coverageReporters": [
      "html",
      "text"
    ]
  }
}
